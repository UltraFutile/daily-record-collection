{
    "type": "better-sqlite3",
    "database": "./db/records.db",
    "synchronize": true,
    "logging": false,
    "entities": [
       "src/entity/**/[!Abstract]*.ts"
    ],
    "migrations": [
       "src/migration/**/*.ts"
    ],
    "subscribers": [
       "src/subscriber/**/*.ts"
    ],
    "cli": {
       "entitiesDir": "src/entity",
       "migrationsDir": "src/migration",
       "subscribersDir": "src/subscriber"
    }
 }